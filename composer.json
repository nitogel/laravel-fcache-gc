{
    "name": "nitogel/laravel-fcache-gc",
    "description": "A Laravel package to backup your application",
    "keywords": [
        "nitogel",
        "clear",
        "cache",
        "laravel"
    ],
    "homepage": "https://github.com/spatie/laravel-backup",
    "license": "MIT",
    "authors": [
        {
            "name": "Kirill Legotin",
            "email": "nitogel@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/console": "~5.8.0",
        "illuminate/filesystem": "~5.8.0",
        "league/flysystem": "^1.0.49"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Nitogel\\LaravelFileCacheGarbageCollector\\": "src"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
